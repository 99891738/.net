@model src.Models.SupportAgent
@{
    Layout = null;
}
<h4>SupportAgent</h4>
<hr />
<div class="row">
    <div class="col-md-10">
        <form onsubmit="return SubmitAddEdit(this)">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="supportAgentId" />
            <input type="hidden" asp-for="organizationId" />
            <div class="form-group">
                <label asp-for="supportAgentName" class="control-label"></label>
                <input asp-for="supportAgentName" class="form-control" />
                <span asp-validation-for="supportAgentName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>



