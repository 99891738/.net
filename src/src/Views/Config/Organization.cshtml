
@{
    ViewData["Title"] = "Organization";
    Layout = "_LayoutConfig";
}

<h2>Organization</h2>
<div class="row">
    <div class="col-md-10">
        <a class="btn btn-success" style="margin-bottom:10px" onclick="ShowPopup('@Url.Action("AddEditOrganization","Config")')"><i class="fa fa-plus"></i> Add New Organization</a>
    </div>
</div>
<div class="row">
    <div class="col-md-10">
        <table id="gridOrganization" name="gridOrganization" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="col-md-8">Organization</th>
                    <th class="col-md-4"></th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Scripts{
    @await Html.PartialAsync("_ValidationScriptsPartial")
    <script src="~/js/views/config/index.js"></script>
}

